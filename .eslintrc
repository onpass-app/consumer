{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["react", "@typescript-eslint", "prettier"],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "no-console": 1,
      "prettier/prettier": 2
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
  }  